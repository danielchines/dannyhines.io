{
  "css.validate": false,
  "editor.formatOnSave": true,
  "editor.lightbulb.enabled": false,
  "editor.fontSize": 13,
  "editor.tabSize": 2,
  "editor.hover.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "files.associations": {
    "*.mdx": "markdown",
  },
  "editor.renderWhitespace": "boundary",
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/coverage": true,
    "**/dist": true,
    "**/build": true,
    "**/.build": true,
    "**/.gh-pages": true
  },
  "prettier.singleQuote": true,
  "prettier.printWidth": 92,
  "cSpell.enableFiletypes": [
    "*"
  ],
  "cSpell.overrides": [{
    "filename": "src/**/{*.mdx}",
    "languageId": "markdown"
  },],
"cSpell.ignorePaths": [
  "package-lock.json",
  "node_modules",
  "vscode-extension",
  ".git/objects",
  ".vscode",
  ".vscode-insiders",
  "public"
],
"cSpell.words": [
  "adviceanimals",
  "bucketdeployment",
  "certificatemanager",
  "clsx",
  "clsxm",
  "copypastas",
  "dankmemes",
  "dotenv",
  "draftkings",
  "endregion",
  "futorology",
  "hardcoding",
  "howmanyblimpsarethere",
  "memeeconomy",
  "mkdir",
  "moneyline",
  "nextjs",
  "pushshift",
  "semibold",
  "sportsbooks",
  "subreddits",
  "tailwindcss",
  "typeof",
  "unstyled",
  "giphy-embed",
]
}