{
  "css.validate": false,
  "editor.formatOnSave": true,
  "editor.lightbulb.enabled": false,
  "editor.fontSize": 13,
  "editor.tabSize": 2,
  "editor.hover.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "typescript.preferences.importModuleSpecifier": "non-relative",
  // "files.associations": {
  //   "*.mdx": "markdown"
  // },
  "editor.renderWhitespace": "boundary",
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/coverage": true,
    "**/dist": true,
    "**/build": true,
    "**/.build": true,
    "**/.gh-pages": true,
    "**/LICENSE.txt": true,
    "**/*.lock": true
  },
  "prettier.singleQuote": true,
  "prettier.printWidth": 92,
  "prettier.proseWrap": "always",
  "cSpell.enableFiletypes": ["*"],
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders",
    "public"
  ],
  "cSpell.words": [
    "adviceanimals",
    "Backtesting",
    "bucketdeployment",
    "certificatemanager",
    "cloudinary",
    "clsx",
    "clsxm",
    "copypastas",
    "dankmemes",
    "dotenv",
    "draftkings",
    "endregion",
    "futorology",
    "giphy-embed",
    "hardcoding",
    "Hinesight",
    "howmanyblimpsarethere",
    "influencers",
    "memeeconomy",
    "mkdir",
    "moneyline",
    "msapplication",
    "nextjs",
    "photoshopped",
    "pushshift",
    "semibold",
    "skeeball",
    "sportsbook",
    "sportsbooks",
    "subreddits",
    "tailwindcss",
    "typeof",
    "umami",
    "unstyled",
    "upvoted",
    "Bitfinex",
    "Hoegner",
    "boxscore",
    "boxscores"
  ],
  "mdx-preview.preview.useVscodeMarkdownStyles": false
}
